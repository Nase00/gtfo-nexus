{
  "plugins": [
    "filenames"
  ],
  "extends": [
    "defaults/configurations/walmart/es6-node"
  ],
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true
  },
  "rules": {
    "strict": 0,
    "quotes": [1, "single"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-invalid-this": 0,
    "valid-jsdoc": 0,
    "prefer-spread": 0,
    "no-console": 0
  },
  "globals": {
    "require": true
  }
}
